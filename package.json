{
  "name": "tiddler-autocommit",
  "version": "1.0.0",
  "description": "",
  "main": "npm run build-all",
  "scripts": {
    "build-all": "npm run build-plugin && npm run build-wiki",
    "build-plugin": "npm run babel && tiddlywiki --output build --build tiddler-autocommit",
    "build-wiki": "tiddlywiki --output build --build index",
    "babel": "babel plugins/tiddler-autocommit/src -d plugins/tiddler-autocommit/lib",
    "serve": "npm run babel && tiddlywiki --listen",
    "postinstall": "(cd plugins/tiddler-autocommit && npm install);"
  },
  "author": "Ashlin Duncan",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/plugin-transform-modules-commonjs": "^7.10.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "simple-git": "^2.6.0"
  }
}
